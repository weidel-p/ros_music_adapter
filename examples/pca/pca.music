stoptime=1000.
rtf=1.0
music_timestep=0.001
[sensor_laser]
  binary=../../ros_sensor_adapter
  args=
  np=1
  ros_topic=/Pioneer3AT/laserscan
  message_type=Laserscan
  sensor_update_rate=20
  ros_node_name=ros_sensor_laser
[encoder_laser]
  binary=../../nef_encoder
  args=
  np=1
  rate_min=1
  rate_max=30
[pca_laser]
  binary=../../pca.py
  args=
  np=1
sensor_laser.out->encoder_laser.in[100]
encoder_laser.out->pca_laser.in[60]
